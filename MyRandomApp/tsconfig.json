{
  "compilerOptions": {
    "noEmitOnError": true,
    "noImplicitAny": true,
    "removeComments": false,
    "module": "es2015",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "suppressImplicitAnyIndexErrors": true,
    "lib": [ "es2015", "dom" ],
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
